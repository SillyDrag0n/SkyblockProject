@model IEnumerable<SkyblockProject.Models.Collection>

    @{
    ViewData["Title"] = "Index";
    }

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="icon" href="..\Images/Icons/logo.png">
    <title>HPSB-Collections</title>
    <div class="navbar">
        <div class="logo"></div>
        <a href="Home">Startseite</a>
        <a id="active" href="Collections">Sammlungen</a>
        <a href="Islands">Inseln</a>
        <a href="Fairysoul">Feenseelen</a>
    </div>
    <div class="main">
        <h1>Sammlungen und Beschreibungen</h1>
        <p>
            Sammlungen sind eine Möglichkeit, neue Craftingrezepte freizuschalten,
            Gegenstände zu tauschen oder Belohnungen für das Sammeln eines bestimmten Gegenstands zu erhalten.
        </p>

        <!-- Farming Collection -->

        <div class="overcollection">
            <h2>Farming Sammlung</h2>
            <div><img src="~/css/Images/Icons/FarmingCollectionIcon.png" alt="Farming Collection Icon" /></div>
            <p>
                Die Farming Sammlung enthält folgende Items. Durch das sammeln dieser Items erhöht sich deren
                Sammlungslevel und man schaltet jegliche Rezept, Items, etc. frei.
            </p>
        </div>

        <!-- Kaktus Collection -->

        <div class="collectionhead">
            <h3>Kaktus Sammlung</h3>
            <div>
                <img src="~/css/Images/Collections/CactusCollection.png" alt="Kaktus Collection Icon" />
            </div>
            <p>
                Das Kaktus Sammlungslevel wird beim Farmen von Kakteen erhöht.<br>
                Kakteen lassen sich vorallem auf der Pilzinsel finden.
            </p>
        </div>

        <table class="collection-table">
            <thead>
                <tr>
                    <th>
                        Sammlungslvl
                    </th>
                    <th>
                        Benötigte Erfahrungspunkte
                    </th>
                    <th>
                        Belohnung
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Where(i => i.CollectionTitle == "Kaktus")) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.CollectionLevel)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CollectionNeededExp)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CollectionUnlock)
                    </td>
                </tr>
        }
            </tbody>
        </table>
